<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.com.mvptestapp.icarasia">

    <application
        android:name="my.com.mvptestapp.icarasia.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="my.com.mvptestapp.icarasia.main.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="my.com.mvptestapp.icarasia.main.MainActivity"/>
        <activity android:name="my.com.mvptestapp.icarasia.loginSignUp.LoginSignUpActivity"/>
    </application>
</manifest>